:name: Dendrophone CM4
using sysbus
mach create "dendrophone"
machine LoadPlatformDescription @platforms/cpus/bcm2711.repl
machine LoadPlatformDescription @platforms/cpus/bcm2711-peripherals.repl

showAnalyzer sysbus.uart0
showAnalyzer sysbus.spi0

$bin=@buildroot/output/images/Image
$dtb=@buildroot/output/images/bcm2711-rpi-cm4.dtb

sysbus LoadELF $bin
sysbus LoadDTB $dtb

start
